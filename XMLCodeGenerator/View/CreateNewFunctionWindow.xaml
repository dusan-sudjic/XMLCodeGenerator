<Window x:Class="XMLCodeGenerator.View.CreateNewFunctionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XMLCodeGenerator.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="New Function" Height="190" Width="300">
    <StackPanel>
        <Label HorizontalAlignment="Center" Margin="0 20 0 0">Enter a name for your new function</Label>
        <TextBox Margin="30 10 30 0" Height="30" x:Name="textBox" TextAlignment="Center" FontSize="13" VerticalContentAlignment="Center"></TextBox>
        <Button Margin="100 10 100 10" Click="SubmitClick">Submit</Button>
    </StackPanel>
</Window>
